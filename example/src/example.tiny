func onText(txt: str) {
	self()->fetch(1)->text(txt)
} 

func btn() {
	prompt("Enter Text", "Text", "onText")
}

func main() {
	z(self_menu(), 999)

	Scale9Sprite(200.0, 100.0, "GJ_square01.png")
		->pos(0.0, 20.0)

	Label("More Text", "bigFont.fnt")
		->pos(0.0, 47.0)
		->tag(1)
		->scale(0.8)

	Button(Sprite("edit_upBtn_001.png"), "btn")
		->pos(0.0, 10.0)
		->scale(0.6)
}
